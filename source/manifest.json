{
  "name": "Preview It!",
  "description": "a chrome extension to preview pages in popup small window",
  "version": "0.1",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "https://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking"
  ], 
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js","main.js"]
    }
  ],
  "manifest_version": 2
}
